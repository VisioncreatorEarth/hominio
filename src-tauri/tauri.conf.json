{
  "$schema": "../../../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Hominio",
  "version": "0.1.0",
  "identifier": "com.hominio.app",
  "build": {
    "frontendDist": "../build",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "bun dev",
    "beforeBuildCommand": "bun run build"
  },
  "app": {
    "windows": [
      {
        "title": "Hominio",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "minimumSystemVersion": "10.13",
      "frameworks": [],
      "entitlements": null,
      "exceptionDomain": "",
      "signingIdentity": null,
      "providerShortName": null
    },
    "resources": [
      "../static/pglite-assets/*",
      "../build/pglite-assets/*",
      "../build/_app/immutable/assets/*.wasm",
      "../build/_app/immutable/assets/*.data"
    ]
  },
  "plugins": {
    "fs": {
      "requireLiteralLeadingDot": false
    }
  }
}